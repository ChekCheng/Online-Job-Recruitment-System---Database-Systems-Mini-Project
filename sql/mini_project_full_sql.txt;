mysql> CREATE TABLE INTERVIEW
    -> (
    -> SELECTION_ID VARCHAR(10) NOT NULL,
    -> FOREIGN KEY(SELECTION_ID) REFERENCES SELECTION_PROCESS(SELECTION_ID),
