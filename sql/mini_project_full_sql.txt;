    -> FOREIGN KEY(TEST_ID) REFERENCES APPLICATION_TEST(TEST_ID),
    -> PRIMARY KEY(SELECTION_ID,TEST_ID),
    -> APPOINTMENT_TIME TIME,
    -> QUESTION VARCHAR(100)
    -> );
